{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
            "baseUrl": "./",
            "paths": {
                "~assets/*": [
                    "assets/*"
                ],
                "~components/*": [
                    "src/components/*"
                ],
                "~utils/*": [
                    "src/utils/*"
                ],
                "~/*": [
                    "src/*"
                ],
                "~root/*": [
                    "./*"
                ],
            }
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts"
, "src/components/__tests__/StyledText-test.js"    ]
}
